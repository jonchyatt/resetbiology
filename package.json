    "googleapis": "^159.0.0",
    "lucide-react": "^0.542.0",
    "nanoid": "^5.1.5",
    "next": "15.5.2",
    "playwright": "^1.55.1",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sqlite3": "^5.1.7",
    "stripe": "^18.5.0",
